not(released_at(blocked1(),0)).


% !ReleasedAt(Blocked2(),0).
not(released_at(blocked2(),0)).


:-was_s_l('/pack/logicmoo_ec/ext/ec_sources/examples/ecasp/StuffyRoom40-ea.e',31).
