holds_at(not(blocked1()),0).


% !HoldsAt(Blocked2(),0).
holds_at(not(blocked2()),0).


:-was_s_l('/pack/logicmoo_ec/ext/ec_sources/examples/ecasp/StuffyRoom40-ea.e',33).
