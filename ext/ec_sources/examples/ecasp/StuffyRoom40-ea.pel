% (ReleasedAt(Stuffy(),time)).
:-was_s_l('/pack/logicmoo_ec/ext/ec_sources/examples/ecasp/StuffyRoom40-ea.e',25).
released_at(stuffy(),Time).


:-was_s_l('/pack/logicmoo_ec/ext/ec_sources/examples/ecasp/StuffyRoom40-ea.e',27).
