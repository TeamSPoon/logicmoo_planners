diff(Xcoord, 2), not((equals(Xcoord, 3), equals(Ycoord, 1), equals(Time, 2))) ->
    (   holds_at(neg(location(g1, y, Xcoord, Ycoord)), Time)
    ;   equals(Xcoord, 1),
        equals(Ycoord, 1),
        equals(Time, 0),
        equal(x, y)
    ).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/OneScreen.e:45
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/OneScreen.e:45
 reactive_rule([ diff(Xcoord, 2),
                   not((equals(Xcoord, 3), equals(Ycoord, 1), equals(Time, 2)))
                 ],
                 [  (at(not(location(g1, y, Xcoord, Ycoord)), Time);equals(Xcoord, 1), equals(Ycoord, 1), equals(Time, 0), equal(x, y))
                 ]).

