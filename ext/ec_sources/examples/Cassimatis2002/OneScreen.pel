%; ycoord1=ycoord2-1).
%; only allow right angle movements
% [grid,object,xcoord1,ycoord1,xcoord2,ycoord2,time]
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/PolySpace.e:111
% Happens(Move(grid,object,xcoord1,ycoord1,xcoord2,ycoord2),time) ->
% HoldsAt(Location(grid,object,xcoord1,ycoord1),time) &
% ((xcoord1=xcoord2 & (ycoord1=ycoord2+1 | ycoord1=ycoord2-1)) |
%  (ycoord1=ycoord2 & (xcoord1=xcoord2+1 | xcoord1=xcoord2-1))).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/PolySpace.e:114
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/PolySpace.e:114
happens(move(Grid, Object, Xcoord1, Ycoord1, Xcoord2, Ycoord2), Time) ->
    holds_at(location(Grid, Object, Xcoord1, Ycoord1),
             Time),
    (   equals(Xcoord1, Xcoord2),
        (   equals(Ycoord1, Ycoord2+1)
        ;   equals(Ycoord1, Ycoord2-1)
        )
    ;   equals(Ycoord1, Ycoord2),
        (   equals(Xcoord1, Xcoord2+1)
        ;   equals(Xcoord1, Xcoord2-1)
        )
    ).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/PolySpace.e:114
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/PolySpace.e:114
 reactive_rule([ happens(move(Grid,
                                Object,
                                Xcoord1,
                                Ycoord1,
                                Xcoord2,
                                Ycoord2),
                           Time)
                 ],
                 [ holds(location(Grid,
                                  Object,
                                  Xcoord1,
                                  Ycoord1),
                         Time),
                    (equals(Xcoord1, Xcoord2), (equals(Ycoord1, Ycoord2+1);equals(Ycoord1, Ycoord2-1));equals(Ycoord1, Ycoord2), (equals(Xcoord1, Xcoord2+1);equals(Xcoord1, Xcoord2-1)))
                 ]).


