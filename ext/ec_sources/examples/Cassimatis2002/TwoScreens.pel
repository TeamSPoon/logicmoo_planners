% [xcoord,ycoord,time]
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/TwoScreens.e:41
% xcoord!=% 2 & xcoord!=4 & !(xcoord=1 & ycoord=1 & time=0) ->
% !HoldsAt(Location(G1,X,xcoord,ycoord),time) |
% xcoord=5 & ycoord=1 & time=4 & Equal(X,Y).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/TwoScreens.e:43
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/TwoScreens.e:43
diff(Xcoord, 2), diff(Xcoord, 4), not((equals(Xcoord, 1), equals(Ycoord, 1), equals(Time, 0))) ->
    (   holds_at(neg(location(g1, x, Xcoord, Ycoord)), Time)
    ;   equals(Xcoord, 5),
        equals(Ycoord, 1),
        equals(Time, 4),
        equal(x, y)
    ).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/TwoScreens.e:43
% From /pack/logicmoo_ec/ext/ec_sources/examples/Cassimatis2002/TwoScreens.e:43
 reactive_rule([ diff(Xcoord, 2),
                   diff(Xcoord, 4),
                   not((equals(Xcoord, 1), equals(Ycoord, 1), equals(Time, 0)))
                 ],
                 [  (at(not(location(g1, x, Xcoord, Ycoord)), Time);equals(Xcoord, 5), equals(Ycoord, 1), equals(Time, 4), equal(x, y))
                 ]).


