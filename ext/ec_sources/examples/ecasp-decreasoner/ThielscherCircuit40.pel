% [time]
% Stopped(Light(),time) &
% Initiated(Switch1(),time) &
% Initiated(Switch2(),time) ->
% Happens(LightOn(),time).
stopped(light(), Time), initiated(switch1(), Time), initiated(switch2(), Time) ->
    happens_at(lightOn(), Time).


