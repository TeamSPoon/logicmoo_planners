%; Sigma
% [object,location1,location2,time]
% From /pack/logicmoo_ec/ext/ec_sources/examples/Mueller2006/Chapter10/OneScreen.e:45
% HoldsAt(At(object,location1),time) &
% Adjacent(location1,location2) ->
% Initiates(Move(object,location1,location2),At(object,location2),time).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Mueller2006/Chapter10/OneScreen.e:47
% From /pack/logicmoo_ec/ext/ec_sources/examples/Mueller2006/Chapter10/OneScreen.e:47
holds_at(at(Object, Location1), Time), adjacent(Location1, Location2) ->
    initiates(move(Object, Location1, Location2),
              at(Object, Location2),
              Time).
% From /pack/logicmoo_ec/ext/ec_sources/examples/Mueller2006/Chapter10/OneScreen.e:47
% From /pack/logicmoo_ec/ext/ec_sources/examples/Mueller2006/Chapter10/OneScreen.e:47
reactive_rule([ holds(loc_at(Object,Location1),Time),
		adjacent(Location1,Location2)
	      ],
	      [ initiates(move(Object,
			       Location1,
			       Location2),
			  at(loc_at(Object,Location2),
			     Time))
	      ]).


