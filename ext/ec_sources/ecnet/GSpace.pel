% From /pack/logicmoo_ec/ext/ec_sources/ecnet/GSpace.e:54
%; A precondition axiom states that for an agent to walk
%; from one cell in a grid to another cell, the agent
%; must be at the first cell, the second cell must not
%; be occupied, and the first cell must be adjacent to
%; the second cell:
% [grid,agent,coord1,coord2,coord3,coord4,time]
% From /pack/logicmoo_ec/ext/ec_sources/ecnet/GSpace.e:60
% Happens(GridWalk(grid,agent,coord1,coord2,coord3,coord4),time) ->
% HoldsAt(GridAt(grid,agent,coord1,coord2),time) &
% (!{object} HoldsAt(GridAt(grid,object,coord3,coord4),time)) &
% (coord1=coord3 |
%  coord1=coord3+1 |
%  coord1=coord3-1) &
% (coord2=coord4 |
%  coord2=coord4+1 |
%  coord2=coord4-1).
% From /pack/logicmoo_ec/ext/ec_sources/ecnet/GSpace.e:68
% From /pack/logicmoo_ec/ext/ec_sources/ecnet/GSpace.e:68
happens(gridWalk(Grid, Agent, Coord1, Coord2, Coord3, Coord4), Time) ->
    holds_at(gridAt(Grid, Agent, Coord1, Coord2),
             Time),
    not(exists([Object],
               holds_at(gridAt(Grid,
                               Object,
                               Coord3,
                               Coord4),
                        Time))),
    (   equals(Coord1, Coord3)
    ;   equals(Coord1, Coord3+1)
    ;   equals(Coord1, Coord3-1)
    ),
    (   equals(Coord2, Coord4)
    ;   equals(Coord2, Coord4+1)
    ;   equals(Coord2, Coord4-1)
    ).
% From /pack/logicmoo_ec/ext/ec_sources/ecnet/GSpace.e:68
% From /pack/logicmoo_ec/ext/ec_sources/ecnet/GSpace.e:68
 reactive_rule([ happens(gridWalk(Grid,
                                    Agent,
                                    Coord1,
                                    Coord2,
                                    Coord3,
                                    Coord4),
                           Time)
                 ],
                 [ holds(gridAt(Grid,
                                Agent,
                                Coord1,
                                Coord2),
                         Time),
                   holds(not(gridAt(Grid,
                                    Object,
                                    Coord3,
                                    Coord4)),
                         Time),
                    (equals(Coord1, Coord3);equals(Coord1, Coord3+1);equals(Coord1, Coord3-1)),
                    (equals(Coord2, Coord4);equals(Coord2, Coord4+1);equals(Coord2, Coord4-1))
                 ]).


