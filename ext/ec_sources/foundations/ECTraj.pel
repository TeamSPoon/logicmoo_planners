% From /pack/logicmoo_ec/ext/ec_sources/foundations/ECTraj.e:36
happens(Event, Time), initiates(Event, Fluent, Time), 0<Offset, trajectory(Fluent, Time, Fluent2, Offset), not(clipped(Time, Fluent, Time+Offset)) ->
    holds_at(Fluent2, Time+Offset).
% From /pack/logicmoo_ec/ext/ec_sources/foundations/ECTraj.e:36
% From /pack/logicmoo_ec/ext/ec_sources/foundations/ECTraj.e:36
reactive_rule([ happens(Event,Time),
		initiates(Event,at(Fluent,Time)),
		0 < Offset,
		trajectory(Fluent,
			   Time,
			   not(Not),
			   Offset),
		not(clipped(Time,
			    Fluent,
			    Time + Offset))
	      ],
	      [holds(not(Not),Time+Offset)]).


